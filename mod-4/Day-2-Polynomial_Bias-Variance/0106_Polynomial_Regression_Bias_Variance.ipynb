    "def model_poly(x, y, deg):\n",
    "    poly = PolynomialFeatures(deg)\n",
    "    \n",
    "    x = x.reshape((-1, 1))\n",
    "    x_poly = poly.fit_transform(x)\n",
    "    \n",
    "    lr = LinearRegression()\n",
    "    lr.fit(x_poly, y)\n",
    "    \n",
    "    y_pred = lr.predict(x_poly)\n",
    "    \n",
    "    rss = np.sum((y-y_pred)**2)\n",
    "    \n",
    "    return rss"
   ]
